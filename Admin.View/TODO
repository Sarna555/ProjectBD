void UpdateGroup(string name); // modyfikuje grupę; tworzy nową jak brak w bazie
void UpdateUserPrivelages(List<string>, string login); //aktualizuje uprawnienia userów
void UpdateGroupPrivelages(List<string>,string groupname); // aktualizuje uprawnienia grupy

